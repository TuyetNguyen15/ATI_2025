{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "node": "18", // Đặt là major version (18)
      "env": {
        "EAS_PROJECT_ROOT": "AI_App_FE/SoulMateApp" // Dùng biến môi trường thay cho rootDir
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "node": "18",
      "env": {
        "EAS_PROJECT_ROOT": "AI_App_FE/SoulMateApp"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "node": "18",
      "env": {
        "EAS_PROJECT_ROOT": "AI_App_FE/SoulMateApp"
      }
    }
  },
  "submit": {
    "production": {
    }
  }
}